<?xml version="1.0" encoding="UTF-8"?>
<config xmlns="http://www.w3schools.com"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="localhost ../xsd/etc_config.xsd">
    <modules>
        <SchemaName_ModuleName>
            <depends></depends>
            <version></version>
        </SchemaName_ModuleName>
    </modules>
    
    <global>
        <blocks></blocks>
        <models></models>
        <newproduct></newproduct>
        <resources></resources>
    </global>
    
    <adminhtml>
        <acl></acl>
        <events></events>
        <menu></menu>
        <translate></translate>
    </adminhtml>
    
    <frontend>
        <events>
            <event_name>
                <observers>
                    <observer_name>
                        <type>model</type>
                        <class>Class_Name</class>
                        <method>methodName</method>
                    </observer_name>
                </observers>
            </event_name>
        </events>
        
        <layout></layout>
        <routers></routers>
        <translate></translate>
    </frontend>
    
    <default>
        <config_vars></config_vars>
    </default>
</config>