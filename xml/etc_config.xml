<?xml version="1.0" encoding="UTF-8"?>
<config xmlns="http://magento.com/schema/module/config"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="../xsd/etc_config.xsd">
    <modules>
        <SchemaName_ModuleName>
            <depends></depends>
            <version></version>
        </SchemaName_ModuleName>
    </modules>
    
    <global>
        <blocks></blocks>
        <models></models>
        <newproduct></newproduct>
        <resources></resources>
        <events>
            <checkout_cart_add_product_complete>
                <observers>
                    <observer_name>
                        <class></class>
                        <method></method>
                        <type></type>
                    </observer_name>
                </observers>
            </checkout_cart_add_product_complete>
        </events>
    </global>
    
    <adminhtml>
        <acl></acl>
        <events></events>
        <menu></menu>
        <translate></translate>
    </adminhtml>
    
    <frontend>
        <events>
            <checkout_cart_save_after>
                <observers>
                    <observer_name>
                        <class></class>
                        <method></method>
                        <type></type>
                    </observer_name>
                </observers>
            </checkout_cart_save_after>
        </events>
        
        <layout></layout>
        <routers></routers>
        <translate></translate>
    </frontend>
    
    <default>
        <config_vars></config_vars>
    </default>
</config>
